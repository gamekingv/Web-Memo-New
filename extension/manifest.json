{
    "manifest_version": 2,
    "name": "Web Memo",
    "version": "0.1.4",
    "description": "网页备忘",
    "icons": {
        "64": "dist/memo.png"
    },
    "browser_action": {
        "browser_style": true,
        "default_icon": {
            "16": "dist/memo.png",
            "32": "dist/memo.png",
            "64": "dist/memo.png"
        }
    },
    "background": {
        "page": "dist/background.html"
    },
    "permissions": [
        "storage"
    ],
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self';"
}